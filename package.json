{
  "name": "hylia",
  "version": "0.7.0",
  "description": "A simple Eleventy starter kit to help you have a blog of your own",
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.3",
    "@justinribeiro/lite-youtube": "^0.9.1",
    "@tbranyen/jsdom": "^13.0.0",
    "concurrently": "^4.1.2",
    "html-minifier": "^4.0.0",
    "image-size": "^0.8.3",
    "json-to-scss": "^1.5.0",
    "rimraf": "^3.0.2",
    "semver": "^6.3.0",
    "slugify": "^1.4.0",
    "stalfos": "github:hankchizljaw/stalfos#c8971d22726326cfc04089b2da4d51eeb1ebb0eb"
  },
  "devDependencies": {
    "bl": "^3.0.0",
    "chokidar-cli": "^2.1.0",
    "cross-env": "^5.2.1",
    "csso": "^4.2.0",
    "gulp": "^4.0.2",
    "make-dir-cli": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "purgecss": "^4.0.2",
    "sass": "^1.32.8"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "cross-env NODE_ENV=development npm-run-all clean sass:tokens gulp -p -r gulp:watch eleventy:watch",
    "eleventy:watch": "eleventy --watch --quiet",
    "eleventy": "eleventy --quiet",
    "gulp:watch": "gulp watch",
    "gulp": "gulp",
    "production": "cross-env NODE_ENV=production npm-run-all clean sass:tokens gulp eleventy",
    "sass:tokens": "npx json-to-scss src/_data/tokens.json src/scss/_tokens.scss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hankchizljaw/hylia.git"
  },
